<h1>Edit Description Id: <%= @url.id %></h1>
<div class="form-group">
<%= form_for @url, url: url_path(@url), method: :patch do |f| %>
  <%= f.label :description %>
  <%= f.text_field :description %>
</div>
<div class="form-group">
    <%= f.label :image %>
    <%= f.file_field :image, class: 'form-control' %>
</div>
<div class="form-group">
  <%= f.submit %>
</div>
<% end %>
<%= link_to 'index', urls_path, class:'btn btn-secondary' %>